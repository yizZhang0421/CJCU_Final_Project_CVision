<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cjcu.ima105.cvision">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/myicon"
        android:label="@string/app_name"
        android:logo="@mipmap/myicon"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/myicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MarketItemDescription"></activity>
        <activity android:name=".Market" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Frame" />
        <activity android:name=".CapcityDistributeDetail" />
        <activity android:name=".Home" />
        <activity android:name=".ModelList" />
        <activity android:name=".PredictCamera" />
        <activity android:name=".ModelPage" />
        <activity android:name=".SignOut" />
        <activity android:name=".LabelPage" />
        <activity android:name=".CreateImageCamera" />
        <activity android:name=".ProgressList" />
        <activity android:name=".ResultPage" />
    </application>

</manifest>